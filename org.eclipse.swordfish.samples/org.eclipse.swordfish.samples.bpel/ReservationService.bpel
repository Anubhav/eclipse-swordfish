<!-- ReservationService BPEL Process [Generated by the Eclipse BPEL Designer] -->
<bpel:process name="ReservationService"
         targetNamespace="http://eclipse.org/swordfish/bpel/reservation"
         suppressJoinFailure="yes"
         xmlns:tns="http://eclipse.org/swordfish/bpel/reservation"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:art="http://bpel.samples.swordfish.eclipse.org"         
         xmlns:boo="http://booking.samples.swordfish.eclipse.org"
         xmlns:sto="http://storage.samples.swordfish.eclipse.org">

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://booking.samples.swordfish.eclipse.org" location="BookingService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import namespace="http://storage.samples.swordfish.eclipse.org" location="ReservationStorageService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="ReservationServiceArtifacts.wsdl" namespace="http://bpel.samples.swordfish.eclipse.org" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="provider" partnerLinkType="art:BookingServicePLT" myRole="provider"></bpel:partnerLink>
        <bpel:partnerLink name="consumer" partnerLinkType="art:ReservationStoragePLT" partnerRole="provider"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        
        
        <bpel:variable name="providerRequest" messageType="sto:createReservation"></bpel:variable>
        <bpel:variable name="providerResponse" messageType="sto:createReservationResponse"></bpel:variable>
        <bpel:variable name="consumerResponse" messageType="res:addReservationResponse"></bpel:variable>
        <bpel:variable name="consumerRequest" messageType="res:addReservation"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in ReservationService.wsdl 
             -->
        
        <bpel:receive name="receiveReservationData" partnerLink="provider"
                 createInstance="yes" operation="createReservation" portType="sto:BookingService" variable="providerRequest"/>
        
        <!-- Generate reply to synchronous request -->
        
        <bpel:assign validate="no" name="AssignReservationData">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:addReservation xmlns:tns="http://booking.samples.swordfish.eclipse.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
</tns:addReservation>
</bpel:literal>
                </bpel:from>
                <bpel:to variable="consumerRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="providerRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[flight]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="consumerRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[reservation/flight]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="providerRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[passengers[1]]]>
                    </bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="consumerRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[reservation/passengers[1]]]>
                    </bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve">1</bpel:literal>
                </bpel:from>
                <bpel:to part="parameters" variable="consumerRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[reservation/id]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeStoreReservation" partnerLink="consumer" operation="addReservation" inputVariable="consumerRequest" outputVariable="consumerResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignReservation">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:createReservationResponse xmlns:tns="http://storage.samples.swordfish.eclipse.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <return></return>
</tns:createReservationResponse>
</bpel:literal>
                </bpel:from>
                <bpel:to variable="providerResponse" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="consumerResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[return]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="providerResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[return]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            
        </bpel:assign>
        <bpel:reply name="replyReservation" partnerLink="provider" operation="createReservation" portType="sto:BookingService" variable="providerResponse"/>
        
    </bpel:sequence>
</bpel:process>

