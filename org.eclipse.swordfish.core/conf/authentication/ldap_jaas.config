sopware.auth-id-password {
    org.sopware.security.login.ldap.LDAPLoginModule REQUIRED
    auth-type=auth-id-password
    container-type=standalone;
};
sopware.auth-principal {
    org.sopware.security.login.ldap.LDAPLoginModule REQUIRED
    auth-type=auth-principal;
};
sopware.auth-token-checking {
    org.sopware.security.login.ldap.LDAPLoginModule REQUIRED
    auth-type=auth-token-checking
    container-type=standalone
    reauthentication=true;
};