<mbeans-descriptors> 
    
    <mbean    name="EndpointManager"
        domain="sbb"
        type="org.eclipse.swordfish.core.components.endpointmanager.impl.EndpointManagerBean"
        description="SBB Kernel Instrumenation">
       
        <attribute  name="ParticipantMapping"
            description="List of endpoints currently activated with the corresponding participant identity"
            type="java.lang.String[]"
            writeable="false"/>
            
        <attribute  name="EndpointCount"
            description="Number of currently opened endpoints on the internal NMS channel"
            type="java.lang.Integer"
            writeable="false"/>
        
    </mbean>
    
</mbeans-descriptors>
