<mbeans-descriptors> 
    
    <mbean    name="Referer"
        domain="sbb"
        type="org.eclipse.swordfish.core.management.instrumentation.Referer"
        description="Dummy resource for testing">
        
        <attribute  name="Attribute"
            description="Test attribute"
            type="java.lang.String"/>
        
        <attribute  name="Children"
            description="Reference to child element"
            type="javax.management.ObjectName[]"
            writeable="false"/>

        <attribute  name="Collection"
            description="Collection-type attribute"
            type="java.util.ArrayList"
            writeable="true"/>

        <attribute  name="Map"
            description="Map-type attribute"
            type="java.util.HashMap"
            writeable="true"/>
        
        <attribute  name="SingleChild"
            description="Reference to child element"
            type="javax.management.ObjectName"
            writeable="true"/>
            
    </mbean>
    
</mbeans-descriptors>
